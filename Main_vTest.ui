   <string notr="true">/* GLOBAL THEME */
QMainWindow {
	background-color: #1e1e1e;
	color: #e0e0e0;
}
/* MENU BAR STYLING */
QMenuBar {
	background-color: #2d2d2d;
	color: #f0f0f0;
	border-bottom: 1px solid #3e3e3e;
}

QMenuBar::item {
	background-color: transparent;
	padding: 4px 10px;
}

QMenuBar::item:selected {
	background-color: #3498db;
	color: white;
}

QMenu {
	background-color: #2d2d2d;
	color: #f0f0f0;
	border: 1px solid #3e3e3e;
}

QMenu::item:selected {
	background-color: #3498db;
	color: white;
}

QWidget {
	font-family: &quot;Segoe UI&quot;, sans-serif;
	font-size: 10pt;
	color: #e0e0e0;
}

/* GROUP BOX STYLING */
QGroupBox {
	background-color: #2d2d2d;
	border: 1px solid #3e3e3e;
	border-radius: 6px;
	margin-top: 24px;
	font-weight: bold;
}

QGroupBox::title {
	subcontrol-origin: margin;
	subcontrol-position: top left;
	left: 10px;
	padding: 0 5px;
	color: #3498db;
	background-color: #1e1e1e;
}

/* BUTTONS */
QPushButton {
	background-color: #3a3a3a;
	border: 1px solid #555;
	border-radius: 4px;
	padding: 6px 12px;
	min-height: 25px;
}

QPushButton:hover {
	background-color: #4a4a4a;
	border: 1px solid #777;
}

QPushButton:pressed {
	background-color: #222;
}

/* INPUT FIELDS */
QLineEdit, QComboBox, QTextEdit {
	background-color: #121212;
	border: 1px solid #444;
	border-radius: 3px;
	color: #fff;
	padding: 4px;
	selection-background-color: #3498db;
}

QLineEdit:focus {
	border: 1px solid #3498db;
}

/* DRO / DISPLAY LABELS */
QLabel[frameShape=&quot;1&quot;] {
	background-color: #000000;
	border: 1px solid #555;
	border-radius: 4px;
	padding: 2px;
}

/* DIALOGS */
QMessageBox, QDialog {
    background-color: #f5f5f5;
}

QMessageBox QLabel, QDialog QLabel {
    color: #000000;
    font-weight: normal;
}

QMessageBox QPushButton, QDialog QPushButton {
    background-color: #e0e0e0;
    color: #000000;
    border: 1px solid #999;
}

QMessageBox QPushButton:hover, QDialog QPushButton:hover {
    background-color: #d0d0d0;
}
QComboBox QAbstractItemView {
                background-color: #2d2d2d;   /* Nền màu xám đậm */
                color: #e0e0e0;              /* Chữ màu trắng sáng */
                selection-background-color: #3498db; /* Màu nền xanh khi di chuột vào dòng */
                selection-color: #ffffff;    /* Màu chữ trắng tinh khi được chọn */
                border: 1px solid #3e3e3e;   /* Viền mỏng cho đẹp */
                outline: 0px;                /* Bỏ đường viền nét đứt */
            }
</string>
